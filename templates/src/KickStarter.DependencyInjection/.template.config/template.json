  "author": "Thet Paing Wynn",
  "classifications": ["KickStarter", "Library"],
  "name": "Kick Starter Dependency Injection Extensions",
  "identity": "DotNet.KickStarter.DependencyInjection",
  "shortName": "ksdepext",
  "tags": {
